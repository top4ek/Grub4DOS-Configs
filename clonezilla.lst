title Back to main menu -->
  ls /usbdrive.tag || find --set-root --ignore-cd /usbdrive.tag
  configfile /menu.lst

title Clonezilla Ubuntu live
  kernel /miscboot/clonezilla/vmlinuz  boot=live config noswap edd=on nomodeset vga=normal noprompt locales= keyboard-layouts= ocs_live_run="ocs-live-general" ocs_live_extra_param="" ocs_live_batch=no ip=frommedia  nosplash i915.blacklist=yes radeonhd.blacklist=yes nouveau.blacklist=yes vmwgfx.blacklist=yes
  initrd /miscboot/clonezilla/initrd.img

